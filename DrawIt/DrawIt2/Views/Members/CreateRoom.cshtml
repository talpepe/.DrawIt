    @Html.ValidationSummary(true)

    <fieldset>
        <legend>RoomModels</legend>


        <div class="editor-label">
            @Html.LabelFor(model => model.RoomName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.RoomName)
